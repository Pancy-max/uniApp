<view class="zhuce"><view class="zc_title"><text>注册</text><view class="zc_ious"></view></view><view class="form"><input class="form_input" type="text" placeholder-style="font-size: 30rpx;color: #B3B3B3;" placeholder="请输入用户名" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" value="{{username}}" bindinput="__e"/><input class="form_input" type="password" placeholder-style="font-size: 30rpx;color: #B3B3B3;" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><input class="form_input" type="password" placeholder-style="font-size: 30rpx;color: #B3B3B3;" placeholder="请再次输入密码" data-event-opts="{{[['input',[['__set_model',['','repassword','$event',[]]]]]]}}" value="{{repassword}}" bindinput="__e"/><input class="form_input" type="text" placeholder-style="font-size: 30rpx;color: #B3B3B3;" maxlength="11" placeholder="请输入手机号码" data-event-opts="{{[['focus',[['e0',['$event']]]],['input',[['__set_model',['','tel','$event',[]]]]]]}}" value="{{tel}}" bindfocus="__e" bindinput="__e"/><view class="msg"><view class="text">验证码</view><view style="display:flex;align-items:center;"><input type="text" placeholder-style="font-size: 30rpx;color: #B3B3B3;" adjust-position="{{true}}" placeholder="请输入验证码" maxlength="6" data-event-opts="{{[['focus',[['e1',['$event']]]],['blur',[['e2',['$event']]]],['input',[['__set_model',['','sms_code','$event',[]]]]]]}}" value="{{sms_code}}" bindfocus="__e" bindblur="__e" bindinput="__e"/><block wx:if="{{!resend}}"><view data-event-opts="{{[['tap',[['getCode',['$event']]]]]}}" class="code" bindtap="__e">获取验证码</view></block><block wx:if="{{resend}}"><view class="resend">{{"重新获取("+countdown+")"}}</view></block></view></view><text class="errtext">{{errmsg}}</text><button data-event-opts="{{[['tap',[['zhuce',['$event']]]]]}}" class="form_button" bindtap="__e">注册</button></view><view class="zc_text"><text class="dltext1">已有账号？</text><text data-event-opts="{{[['tap',[['theLogin',['$event']]]]]}}" class="dltext2" bindtap="__e">现在登录</text></view></view>