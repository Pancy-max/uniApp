  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.lo_icon .lo_image {
  margin-bottom: 22rpx;
}
.lo_icon .lo_image image {
  width: 124rpx;
  height: 124rpx;
}
.lo_icon .lo_text image {
  width: 196rpx;
  height: 54rpx;
}
.regbutton_login {
  width: 630rpx;
  height: 48rpx;
  text-align: center;
  line-height: 48rpx;
  border: #b2b2b2 1rpx;
  margin: 70rpx auto;
}
.lo_font {
  width: 100%;
  position: absolute;
  bottom: 145rpx;
  font-size: 24rpx;
  color: #808080;
  text-align: center;
}
.button {
  background-color: #65bb25;
  font-size: 32rpx;
