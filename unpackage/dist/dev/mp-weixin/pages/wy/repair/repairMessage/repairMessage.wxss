.btn {
		width: 100%;
		height: 104rpx;
		text-align: center;
		line-height: 104rpx;
		color: #FFFFFF;
		font-size: 34rpx;
		background-color: #68cda2;
		position: fixed;
		bottom: 0;
		left: 0;
}
.box {
	padding: 0px 28rpx;
	background-color: #FFFFFF;
}
.head{
		font-size: 44rpx;
		color: #333333;
		font-weight: bold;
		padding-top: 12rpx;
		padding-bottom: 38rpx;
		border-bottom: 1px solid #e6e6e6;
		display: flex;
		justify-content: space-between;
		align-items: center;
}
.title {
		font-size: 32rpx;
		color: #333333;
		margin-top: 32rpx;
		font-weight: bold;
}
.msg {
	margin-bottom: 20rpx;
	display: flex;
	justify-content: space-between;
	width: 100%;
	height: 104rpx;
	align-items: center;
	border-bottom: 1px solid #e6e6e6;
}
.text {
		display: flex;
		font-size: 30rpx;
		color: #262626;
}
input{
		text-align: right;
		font-size: 30rpx;
		color: #4d4d4d;
}
.textInput {
	display: flex;
	justify-content: space-between;
	margin-top: 28rpx;
}
.text_left {
	font-size: 30rpx;
	color: #262626;
}
.text_right {
	font-size: 30rpx;
	color: #808080;
}
.repair-message .title {
  margin: 32rpx;
  padding: 20rpx 0;
  font-size: 40rpx;
  font-weight: 700;
  border-bottom: 3rpx solid #e7e8e7;
}
.repair-message .detail {
  margin: 32rpx;
}
.repair-message .detail .user-info-title {
  padding: 20rpx 0;
  font-size: 35rpx;
  font-weight: 700;
}
.repair-message .detail .check-btn {
  display: flex;
  padding: 100rpx;
  border-top: 3rpx solid #e7e8e7;
}
.repair-message .detail .untreat .deal-process .process-msg {
  width: 100%;
  border: 3rpx solid #999999;
  border-radius: 10rpx;
}
.repair-message .detail .untreat .maintainer .input-msg {
  height: 20rpx;
  width: 100%;
  border: 3rpx solid #999999;
  border-radius: 10rpx;
}
.repair-message .m10 {
  margin: 10rpx 0;
}
.repair-message .fs30 {
  font-size: 30rpx;
  color: #6e7069;
}
.repair-message .mb20 {
  margin-bottom: 20rpx;
}
.btn {
  color: #fff;
  width: 100%;
  height: 100rpx;
  background-color: #2273f6;
}
.btnw {
  color: #fff;
  width: 50%;
  margin: 10rpx;
  height: 100rpx;
  background-color: #2273f6;
}
.image {
  margin-top: 10rpx;
  display: flex;
  flex-wrap: wrap;
}
.image .imageitem {
  height: 200rpx;
  width: 200rpx;
  margin: 8rpx;
}
.image .imageitem image {
  height: 200rpx;
  width: 200rpx;
}

