.fee_message {
  margin: 0 30rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #e6e6e6;
  display: flex;
  justify-content: space-between;
}
.fee_tit {
  font-size: 28rpx;
  color: #28B07C;
}
.fee_tit1 {
  font-size: 44rpx;
  font-weight: 700;
}
.fee_message1 {
  margin: 0 30rpx;
  border-bottom: 1rpx solid #e6e6e6;
}
.fee_message1 .fee_radio {
  font-size: 30rpx;
  font-weight: 700;
  margin-top: 30rpx;
}
.fee_message1 .fee_radio1 {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
}
.fee_message1 .fee_radio1 image {
  width: 38rpx;
  height: 38rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.fee_message1 .fee_radio1 .radio_group {
  color: #C8C7CC;
}
.fee_message1 .fee_radio1 .fee_rad {
  display: flex;
  align-items: center;
}
.fee_message1 .fee_radio1 .fee_rad .fee_radio2 {
  height: 100rpx;
  line-height: 100rpx;
}
.fee_message1 .fee_radio1 .star {
  color: red;
  font-size: 25rpx;
  display: inline-block;
  height: 100rpx;
  padding-top: 50rpx;
  margin-left: 5rpx;
}
.fee_text {
  margin: 0 30rpx;
  border-bottom: 1rpx solid #e6e6e6;
  padding-top: 30rpx;
}
.fee_text .text_title {
  display: flex;
  justify-content: space-between;
}
.fee_text .text_title .text_left {
  display: flex;
}
.fee_text .text_title .text_left .text_star {
  color: red;
  font-size: 25rpx;
  margin-left: 5rpx;
}
.fee_text .text_title .text_right {
  color: #C8C7CC;
}
.fee_text textarea {
  margin-top: 20rpx;
  height: 200rpx;
}
.fee_input {
  margin: 0 30rpx;
  display: flex;
  justify-content: space-between;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #e6e6e6;
}
.fee_input input {
  width: 390rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.fee_button {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  border-radius: 0;
}
.status_bar {
  height: 25px;
  width: 100%;
}
.tabbar {
  height: 84rpx;
  display: flex;
  align-items: center;
  padding: 0 22rpx;
}
.tabbar image {
  width: 48rpx;
  height: 48rpx;
}
.tabbar .img {
  margin-left: 18rpx;
}
.selImg {
  width: 36rpx;
  height: 36rpx;
  margin-top: -15rpx;
}
.norSel {
  color: #b3b3b3;
}
.sel {
  color: #4d4d4d;
}
.select {
  display: flex;
}
.select .select_option {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #b3b3b3;
}
.select .selected {
  display: flex;
  font-size: 30rpx;
  color: #808080;
}

