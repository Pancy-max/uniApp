.account {
  width: 100%;
}
.account .font_top {
  margin-top: 14rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  padding-bottom: 38rpx;
  box-sizing: border-box;
  font-size: 44rpx;
  color: #333333;
  border-bottom: 2rpx solid #E6E6E6;
  font-weight: 900;
}
.account .font_image {
  height: 116rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-bottom: 2rpx solid #E6E6E6;
  font-size: 30rpx;
  color: #262626;
}
.account .font_image .icon {
  height: 88rpx;
  width: 88rpx;
  border-radius: 6rpx;
}
.account .font_image .icon1 {
  margin-left: 24rpx;
  margin-right: 26rpx;
  height: 24rpx;
  width: 24rpx;
}
.fontImage {
  margin-left: 24rpx;
  margin-right: 26rpx;
  height: 116rpx;
}
.font_icon {
  display: 1;
  display: flex;
  align-items: center;
}
.popups {
  border-radius: 20rpx;
  height: 350rpx;
  width: 100%;
  padding: 0 28rpx;
  background-color: #FFFFFF;
}
.popups .paishe {
  font-size: 34rpx;
  color: #28b07c;
  border-bottom: 1rpx solid #e6e6e6;
  height: 108rpx;
  line-height: 108rpx;
  text-align: center;
}
.popups .quxiao {
  height: 134rpx;
  padding-top: 36rpx;
  font-size: 34rpx;
  color: #C8C7CC;
  text-align: center;
}

